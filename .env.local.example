# Supabase Configuration
# Get these values from your Supabase project settings (https://app.supabase.com)

# Your Supabase project URL (found in Settings > API)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url

# Your Supabase service role key (found in Settings > API > service_role key)
# IMPORTANT: This is a SECRET key - never commit the actual .env.local file to git!
# This key bypasses Row Level Security and should only be used server-side
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Microsoft Clarity Configuration
NEXT_PUBLIC_CLARITY_PROJECT_ID=your_clarity_project_id
